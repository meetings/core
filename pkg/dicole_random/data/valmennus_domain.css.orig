/** By Jussi **/

    @font-face {
        font-family: "GeoSlab";
        font-style: normal;
        font-weight: normal;
        src: url("/images/tuuma/fonts/235229_0_0.woff") format("embedded-opentype"), url("/images/tuuma/fonts/235229_0_0.woff") format("woff"), url("/images/tuuma/fonts/235229_0_0.ttf") format("truetype");
    }
    h1, h2, h3, h4, h5, h6 {
        color: #000000;
        font-family: 'GeoSlab',sans-serif;
        font-weight: normal;
        text-rendering: optimizelegibility;
    }
    div.content{width:980px;}
    html body{
        background:none !important;
        background-image:url(/images/tuuma/valmennus-taustakuva.jpg) !important;
        background-position: center 173px !important;
        background-repeat: no-repeat !important;
        background-size: cover !important;
    }

    .dcp-header div.header{
        background: none repeat scroll 0 0 #FFFFFF;
        height: 173px;
        overflow: hidden;
        position: relative;
        width: 100%; }
    .dcp-header .inner {
        margin: 0 auto;
        overflow: hidden;
        position: relative;
        width: 980px; }
    .dcp-header div.header .inner {
        border-bottom: 1px solid #1F1E21;
        height: 173px; }
    .dcp-header #logo {
        border: medium none;
        left: 10px;
        position: absolute;
        top: 87px; }
    .dcp-header a, .dcp-header a:active {
        border-bottom: 1px solid #F86300;
        color: #F86300;
        text-decoration: none; }

    div.top-nav{
        position: absolute;
        right: 10px;
        top: 113px;
    }
    div.login-etc{
        position:relative;
        padding-top:20px;
        margin-bottom:20px;
        text-align:right;
    }
    div.dcp-controls ul{
        list-style: none outside none;
        margin: 0;
        padding: 0;
    }
    div.dcp-controls ul li:first-child:before {
        content: "";
        margin: 0;
    }
    div.dcp-controls ul li:before {
        color: #000000;
        content: "// ";
        font-size: 14px;
        margin: 0;
    }
    div.dcp-controls ul li{
        display: inline;
    }
    div.dcp-controls ul li a{
        border: medium none;
        color: #000000;
        font-family: 'GeoSlab';
        font-size: 14px;
        padding: 10px 6px;
        text-decoration: none;
        text-transform: uppercase;
    }
    div.dcp-controls ul li a:hover {
        border: medium none;
        color: #F86300;
    }
    div.dcp-controls  ul li.active a{
        border: medium none;
        color: #F86300;
    }

    #content div.aside {
        float: left;
        margin: 0 0 0 10px;
        width: 320px;
    }
    #page-label {
        margin-bottom: 40px;
    }
    p.page-label {
        background: none repeat scroll 0 0 #F86300;
        color: #FFFFFF;
        display: inline-block;
        font-family: 'GeoSlab';
        font-size: 22px;
        font-weight: 100;
        height: 34px;
        line-height: 34px;
        margin: 0;
        padding: 0 15px;
    }
    div.aside .promo {
        float: left;
        margin: 30px 0 0;
    }
    #content #main-content {
        background-color:#fff;
        float: right;
        margin: 72px 0px 15px 0;
        position: relative;
        width: 635px;
    }
    #content{
        border-bottom:2px solid #999;
        height:250px;
    }

    /* Login stuffs */
    div#light .loginHeader {
        background-color: #000;
        font-weight: bold;
        color:#fff;
    }
    div#light #light_login_button{
        background-color:#F86300;
        color:#fff;
    }
    .loginTitle{
        color:#000;
    }
    #light_login_button{
        font-family:arial;
    }
    div#light {
        border-bottom: 10px solid #000;
        border-top: 5px solid #000;
    }
    .loginLinks a{
        color:#F86300;
    }
    .loginLinks a:hover{
        background:transparent !important;
        color:#F86300;
    }

    .page-intro{
        padding:15px 15px 25px 15px;
    }
    .page-intro h1{
        font-size:15px;
        font-family:'GeoSlab';
        color:#000000 !important;
    }
    .page-intro p{
        color:#000;
    }

    /* Left Menu */
    .events_listing_navigation div.contentItemContainer{
        border:none;
        border-radius:0px;
        background:none;

    }
    .tool_2_column_td_1 div.blueLinks div.verticalContent{
        width:325px;
        margin-bottom:4px;
    }
    a.linkBar.selected, a.linkBar.selected:hover{
        background-color:#000000;
        background-image:none !important;
        border:none;
        color:#ffffff !important;
        font-family:'GeoSlab';
        font-weight:normal;
        padding:6px;
    }
    a.linkBar{
        background-color:#f86300;
        border:none;
        color:#ffffff !important;
        font-family:'GeoSlab';
        font-weight:normal;
        padding:6px;
    }
    a.linkBar:hover{
        background-color:#000000;
        background-image:none !important;
        border:none;
        color:#ffffff !important;
        font-family:'GeoSlab';
        font-weight:normal;
        padding:6px;
    }
    /* General */
    div.toolContainer{
        padding:0px;
    }
    td.tool_2_column_td_1{
        padding-left:0px;
    }
    div.events_listing_navigation{
        background-color:transparent !important;
        margin-top:0px;
        padding-left:0px;
        margin-left:0px;
        margin-bottom:0px;
    }
    div.tool_2_column_div_1 div.events_listing_tags{
        margin-top:0px;
        margin-left:0px;
        background:#fff;
        width:285px;
        padding:20px !important;
    }
    div.events_listing_tags span.boxLegend{
        display:none !important;
    }
    .event .event-content .tags .tag, .tagPrimary, #event-info .tag, #event-edit .tag{
        color:#999 !important;
        font-size:10px;
        font-family:arial;
        border-radius:0px;
    }
    .tag:hover, .event .event-content .tags .tag:hover, .miniTagCloud a:hover, #event-info .tag:hover, #event-edit .tag:hover{
        color:#fff !important;
        background:#F86300 !important;
    }

    /* Center content */
    .toolContainer{
        width:980px;
    }
    .tool,.toolContainer{
        background:none;
    }
    div.toolContent{
        margin-right:0px;
    }
    div.tool_2_column_div_2 div.toolContentBox{
        background-color:#fff;
        margin-top:0px;
        margin-left:10px;
        width:608px;
    }
    .tool_2_column_div_2{
        padding-right:0px;
    }
    div.bt{display:none;}
    div.bb{display:none;}
    div.contentItemContainer{
        padding:0px;
    }
    div.contentItemContainer span.boxLegend{
        height:22px;
        background-color:#000;
        font-size:15px;
        font-family:'GeoSlab';
        color:#fff !important;
    }
    div.contentItemContainer span.boxLegend span.boxLegend2Text{
        padding-left:20px;
        font-size:15px;
        font-family:'GeoSlab';
        color:#fff !important;
        font-weight:normal;
    }
    .yellow-button{
        height:20px;
        line-height:20px;
        font-size:14px;
        background:none;

    }
    .yellow-button span{

        height:20px;
        line-height:20px;
        font-size:14px;
        background:none;
    }
    .event .event-content .title,  .event .event-content .title a{
        color:#000 !important;
    }
    .event .right .social-media-tag, #event-info .social-media-tag{
        color:#ccc !important;
        background:#444 !important;
        border-radius:0px;
        font:12px Arial;
        font-weight:normal;
        padding:5px;
    }
    #event-info .social-media-tag{
        margin:10px 0px;
    }
    .event .event-content .date.live{
        color:#000;
    }
    body.action_events_task_show div.tool{
        background-color:#fff;
    }
    .action_events h1,.action_events h3,.action_events h2{
        color:#000 !important;
    }
    }
    #main-content a{
        color:#F86300 !important;
    }
    body.action_events_task_show .blueLinks a,  body.action_events_task_show .blueLinks a:visited,  body.action_events_task_show .blueLinks a:link,  body.action_events_task_show .blueLinks a:active{
        color:#F86300 !important;
    }

